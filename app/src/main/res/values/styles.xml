<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>



    <style name="SeekBar">

        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:maxHeight">4dp</item>
        <item name="android:progressDrawable">@drawable/progressbar_bg</item>
        <item name="android:minHeight">4dp</item>
        <item name="android:thumb">@drawable/seekbar_thumb</item>
        <item name="android:isScrollContainer">true</item>
        <item name="android:fadeScrollbars">true</item>


    </style>

</resources>
